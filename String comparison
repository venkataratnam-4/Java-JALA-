public class StringComparisonExample {
    public static void main(String[] args) {
        String str1 = "Hello";
        String str2 = "Hello";
        String str3 = "hello";

       
        System.out.println("str1 equals str2: " + str1.equals(str2)); 
        System.out.println("str1 equals str3: " + str1.equals(str3));
    }
}
