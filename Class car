class Car{
    private String color;
    private String name;
    
    public Car(String color, String name){
        this.color=color;
        this.name=name;
        System.out.println(color + " is a brand name");
    }
    public void bark(){
        System.out.println(name + " is a brand name");
    }
}
public class Main{
    public static void main(String[] args) {
        Car car1=new Car("Blue","Audi");
        car1.bark();
    }
}
