public class CountEvenOdd {
    public static void main(String[] args) {
        int[] numbers = {10, 21, 32, 43, 54, 65}; 
        int evenCount = 0, oddCount = 0;
 for (int num : numbers) {
            if (num % 2 == 0) {
                evenCount++; 
            } else {
                oddCount++;
            }
        }
 System.out.println("Even numbers: " + evenCount);
        System.out.println("Odd numbers: " + oddCount);
    }
}
